<div id="menu">
	<div class="logo">
		<img src="/images/logo.png" class="black">
		<img src="/images/logo-white.png" class="white">
	</div>
	<div class="menu" id="main-menu">
		<div class="item" data-name="">
			<img src="/icons/lang.svg" width="20" class="black">
			<img src="/icons/lang-white.svg" width="20" class="white">
		</div>
		<div class="item itemActive" onclick="menuScrollToSection(this)" data-name="about">
			ABOUT
		</div>
		<div class="item" onclick="menuScrollToSection(this)" data-name="products">
			PRODUCTS
		</div>
		<div class="item" onclick="menuScrollToSection(this)" data-name="fiberglass">
