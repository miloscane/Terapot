<div id="menu">
	<div class="logo" onclick="window.location.href='/'">
		<img src="/images/logo.png" class="black">
		<img src="/images/logo-white.png" class="white">
	</div>
	<div class="menu" id="main-menu">
		<div class="item" data-name="">
			<img src="/icons/lang.svg" width="20" class="black">
			<img src="/icons/lang-white.svg" width="20" class="white">
		</div>
		<div class="item" onclick="menuScrollToSection(this)" data-name="vision">
			ABOUT
		</div>
		<div class="item" onclick="menuScrollToSection(this)" data-name="products">
			PRODUCTS
		</div>
		<div class="item" onclick="window.location.href='/whyfiberglass'">
			WHY FIBERGLASS
		</div>
		<div class="item" onclick="menuScrollToSection(this)" data-name="contact">
			CONTACT
		</div>
	</div>
	<div class="menu rMenu">
		<div class="item" data-name="">
			<img src="/icons/lang.svg" width="20" class="black">
			<img src="/icons/lang-white.svg" width="20" class="white">
		</div>
		<div class="item rItemHandle" onclick="responsiveMenuOpen(this)">
			<div class="topHandle handle"></div>
			<div class="bottomHandle handle"></div>
		</div>
		<script>
			function responsiveMenuOpen(elem){
				var menuItem = document.getElementById("menu");
				if(menuItem.classList.contains("mainMenuOpened")){
					menuItem.classList.remove("mainMenuOpened");
					if(document.getElementById("site-wrap").style.top=="0px"){
						menuItem.classList.remove("whiteMenu")
					}
				}else{
					menuItem.classList.add("mainMenuOpened");
					if(!menuItem.classList.contains("whiteMenu")){
						menuItem.classList.add("whiteMenu")
					}
				}
			}
		</script>
	</div>
</div>